#parse ($init)

<html class="#language("lang.dir")" dir="#language("lang.dir")" lang="$w3c_language_id">

<head>
  <title>$the_title - $company_name</title>

  $theme.include($top_head_include)

  <!-- Web Fonts -->
  <link href="http://fnt.webink.com/wfs/webink.css/?project=AEC37CF0-6B2D-450D-9777-5062C06CCACA&fonts=58A3DDEA-1A20-AFB9-54E7-882B2382F4AD:f=ProximaNova-SemiboldIt,5A9479E7-B0E0-2DCD-5F27-F7666DF19C0F:f=ProximaNova-LightIt,F5870F1F-48F5-36F5-550E-B276AA09225A:f=ProximaNova-RegularIt,1366810A-C17C-C074-FF25-E81320293740:f=ProximaNovaCond-Semibold,C344819E-7F64-52D2-5903-DE54F5382845:f=ProximaNova-Semibold,58DEFEBD-4A55-477B-3C13-525C6AEBDD7A:f=ProximaNova-Regular,3CE3758E-9A83-BA6E-300A-ACCFD5D3E584:f=ProximaNova-BoldIt,FF4969BA-A10C-C1F1-4CAE-97A4557D103C:f=ProximaNovaCond-Regular,C88A28DF-0112-63D9-25D2-A4EA08DAF75B:f=ProximaNova-Light,382B4521-6228-4C38-41BC-3DE66C7C910A:f=ProximaNova-Bold" rel="stylesheet" type="text/css"/>
	
	<!-- Pre DOM-Ready scripts -->
	<script type="text/javascript" src="$javascript_folder/modernizr.js"></script>
	<script>
	if( !window.html5 ){ window.html5 = {}; } window.html5.shivMethods = false;
	</script>
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
	<script type="text/javascript" src="$javascript_folder/mousepress.js"></script>
	<script type="text/javascript" src="$javascript_folder/polyfiller.js"></script>
	<script type="text/javascript" src="$javascript_folder/polyfills.js"></script>
	<script type="text/javascript" src="$javascript_folder/oo_engine.min.js"></script>
	<script>
	var oo_feedback = new OOo.Ocode({});
	</script>

	<!--[if lte IE 7]>
	<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<script type="text/javascript" src="$javascript_folder/lte-ie7.js"></script>
	<![endif]-->


</head>

<body class="$css_class">

  $theme.include($body_top_include)

  #if ($is_signed_in)
    #dockbar()
  #end

  <a class="invisible" href="#main-content" id="skip-to-content" style="position:absolute;">#language("skip-to-content")</a>

  #parse ("$full_templates_path/header.vm")

  <div id="js-hero-dropdowns" class="hero-unit hero-inverted getHeroDropdownStatus">
    <div class="container">
      <div id="js-shopping-dropdown" class="show">
        <div id="audience_dropdown_alert"></div>
        <form class="clearfix form-personalize form-inline" id="form-personalize">
          <div class="row-fluid">
            <div class="span4">
              <div id="personalization_message" class="new">
                <h1>Hello</h1>
                <p>Tell us who you are and<br /> we'll personalize the website.</p>
              </div>
            </div>
            <div class="span8">
              <div class="navbar tooltip-container">
                <ul role="navigation" class="nav">
                  <li class="text">I'm</li>
                  <li class="dropdown" data-placement="top" data-original-title="Select from the dropdown menu" rel="tooltip">
                    <a href="#" id="personalization_audience_dropdown" class="dropdown-toggle" data-toggle="dropdown" role="button">
                      audience <b class="caret"></b></a>
                    <ul aria-labelledby="toggle-personalize" class="dropdown-menu" id="dropdown-personalize">
                      <li id="option"><a>value</a></li>
                    </ul>
                  </li>
                </ul>
                <ul class="nav">
                  <li class="text">in</li>
                  <li class="input" data-placement="top" data-original-title="Enter your ZIP code" rel="tooltip"><input class="zip" id="zip-personalize" type="text" size="10" placeholder="ZIP code" value="zipcode"></li>
                  <li class="button"><button class="btn" id="audience_region_button" type="submit">Go</button></li>
                </ul>
              </div>
            </div>
          </div>
        </form>
      </div>
      <div id="js-search-dropdown" class="hide">
        <form class="clearfix form-personalize form-inline" id="form-search">
          <div class="row-fluid">
            <div class="span5">
              <h1>Search</h1>
              <p>The fastest, easiest way to<br/> find what you need.</p> 
            </div>
            <div class="span7">
              <div class="navbar">
                <ul class="nav">
                  <li><input class="search" id="search-query" type="text" size="12" placeholder="Search Regence.com"></li>
                  <li><button class="btn" id="search_button" type="submit">Go</button></li>
                </ul>
              </div>
            </div>
          </div>
        </form>
      </div>
    </div> <!-- /container -->
  </div>

  <div class="container" id="main-content">
    <nav class="site-breadcrumbs" id="breadcrumbs">
      #breadcrumbs()
    </nav>
    <div class="article article-left-sidebar row-fluid">
      <div class="content main-content">
        <div class="page-header">
          <h1>$the_title</h1>
        </div>
        <section id="content">
          #if ($selectable)
            $theme.include($content_include)
          #else
            $portletDisplay.recycle()

            $portletDisplay.setTitle($the_title)

            $theme.wrapPortlet("portlet.vm", $content_include)
          #end
        </section>
      </div>
    </div>
    <!--[if lte IE 8]>
    <hr/>
    <div class="row-fluid">
      <div class="alert alert-block fade in">
        <button data-dismiss="alert" class="close" type="button">Ã—</button>
        <h4 class="alert-heading">You're using an old version of Internet Explorer</h4>
        <p>We've designed this site to work best with the latest versions of Internet Explorer, Safari, Firefox, Chrome and Opera. Consider upgrading your browser to the latest version for enhanced functionality and improved security.</p>
      </div>
    </div>
    <![endif]-->
  </div> <!-- (main) /container -->

  #if ($has_navigation || $is_signed_in)
    #parse ("$full_templates_path/navigation_primary.vm")
    #parse ("$full_templates_path/navigation_secondary.vm")
  #end

  #parse ("$full_templates_path/footer.vm")

  $theme.include($body_bottom_include)
  
  
  <script type="text/javascript" src="$javascript_folder/bootstrap.min.js"></script>
  <script type="text/javascript" src="$javascript_folder/plugins.js"></script>
  <script type="text/javascript" src="$javascript_folder/script.js"></script>

</body>

$theme.include($bottom_include)

</html>